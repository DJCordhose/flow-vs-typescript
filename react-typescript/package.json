    "isomorphic-fetch": "^2.2.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "expect": "^1.20.2",
    "jsdom": "^9.4.1",
    "jsdom-global": "^2.0.0",
    "mocha": "^2.5.3",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "ts-node": "latest",
    "tslint": "next",
    "tslint-microsoft-contrib": "^2.0.8",
    "typescript": "next",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
