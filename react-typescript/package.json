{
  "name": "react-typescript",
  "version": "0.0.1",
  "scripts": {
    "server": "node src/server",
    "weback": "webpack --watch",
    "devserver": "webpack-dev-server --progress --colors --hot --content-base public",
    "tslint": "tslint -r node_modules/tslint-microsoft-contrib -c tslint.json src/index.tsx",
    "test": "mocha test -r jsdom-global/register --compilers ts:ts-node/register --recursive",
    "test:watch": "npm run test -- --watch"
